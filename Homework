#include <iostream>
#include <string>

int main()
{
    std::cout << "enter number: ";
    int number{};
    std::cin >> number;

    std::cout << " Enter name: ";
    std::string name{};
    std::getline(std::cin >> std::ws, name);

    std::cout << "My name is " << name << '\n' << number << std::endl;


    ;return EXIT_SUCCESS;
}
